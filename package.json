{
  "name": "vue-blog",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "node-build": "babel-node ./serve -d dist-serve",
    "node-dev": "npm run node-build && node ./dist-serve/server.js dev",
    "node-prod": "node ./serve/server.js prod"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cors": "^2.8.4",
    "element-ui": "^2.4.6",
    "express": "^4.16.3",
    "mockjs": "^1.1.0",
    "morgan": "^1.10.0",
    "mysql": "^2.16.0",
    "qs": "^6.9.4",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.0",
    "@babel/register": "^7.17.7",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
